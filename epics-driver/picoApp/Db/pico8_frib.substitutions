file "pico8_frib_reg_chan.template"
{
{P="\$(SYS):\$(D)_CH0:", NAME="\$(NAME)", CORR="0x300c0", CTRL="0x300e0", BIT="0"}
{P="\$(SYS):\$(D)_CH1:", NAME="\$(NAME)", CORR="0x300c4", CTRL="0x300e0", BIT="16"}
{P="\$(SYS):\$(D)_CH2:", NAME="\$(NAME)", CORR="0x300c8", CTRL="0x300e4", BIT="0"}
{P="\$(SYS):\$(D)_CH3:", NAME="\$(NAME)", CORR="0x300cc", CTRL="0x300e4", BIT="16"}
{P="\$(SYS):\$(D)_CH4:", NAME="\$(NAME)", CORR="0x300d0", CTRL="0x300e8", BIT="0"}
{P="\$(SYS):\$(D)_CH5:", NAME="\$(NAME)", CORR="0x300d4", CTRL="0x300e8", BIT="16"}
{P="\$(SYS):\$(D)_CH6:", NAME="\$(NAME)", CORR="0x300d8", CTRL="0x300ec", BIT="0"}
{P="\$(SYS):\$(D)_CH7:", NAME="\$(NAME)", CORR="0x300dc", CTRL="0x300ec", BIT="16"}
}

file "pico8_frib_chan_trip.db" {
pattern
{P,                    NAME,       LO1,  HI1,  LO2,  HI2,  LO3,  HI3,  LO4,  HI4,  SPK1, SPK2, NOKMSK, NOK  }
{"\$(SYS):\$(D)_CH0:", "\$(NAME)", 0xB0, 0xB1, 0xB2, 0xB3, 0xB4, 0xB5, 0xB6, 0xB7, 0xF0, 0xF1, 0xA0,   0xA8 }
{"\$(SYS):\$(D)_CH1:", "\$(NAME)", 0xB8, 0xB9, 0xBA, 0xBB, 0xBC, 0xBD, 0xBE, 0xBF, 0xF2, 0xF3, 0xA1,   0xA9 }
{"\$(SYS):\$(D)_CH2:", "\$(NAME)", 0xC0, 0xC1, 0xC2, 0xC3, 0xC4, 0xC5, 0xC6, 0xC7, 0xF4, 0xF5, 0xA2,   0xAA }
{"\$(SYS):\$(D)_CH3:", "\$(NAME)", 0xC8, 0xC9, 0xCA, 0xCB, 0xCC, 0xCD, 0xCE, 0xCF, 0xF6, 0xF7, 0xA3,   0xAB }
{"\$(SYS):\$(D)_CH4:", "\$(NAME)", 0xD0, 0xD1, 0xD2, 0xD3, 0xD4, 0xD5, 0xD6, 0xD7, 0xF8, 0xF9, 0xA4,   0xAC }
{"\$(SYS):\$(D)_CH5:", "\$(NAME)", 0xD8, 0xD9, 0xDA, 0xDB, 0xDC, 0xDD, 0xDE, 0xDF, 0xFA, 0xFB, 0xA5,   0xAD }
{"\$(SYS):\$(D)_CH6:", "\$(NAME)", 0xE0, 0xE1, 0xE2, 0xE3, 0xE4, 0xE5, 0xE6, 0xE7, 0xFC, 0xFD, 0xA6,   0xAE }
{"\$(SYS):\$(D)_CH7:", "\$(NAME)", 0xE8, 0xE9, 0xEA, 0xEB, 0xEC, 0xED, 0xEE, 0xEF, 0xFE, 0xFF, 0xA7,   0xAF }
}
