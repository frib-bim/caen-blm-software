
file "pico8_base.template"
{
        {P="\$(SYS):\$(D)_CTRL:", NAME="\$(NAME)", NELM="\$(NELM=1024)"}
}

file "pico8_chan.template"
{
        {N=0, P="\$(SYS):\$(D)_CH$(N):", NAME="\$(NAME)", NELM="\$(NELM=1024)"}
        {N=1, P="\$(SYS):\$(D)_CH$(N):", NAME="\$(NAME)", NELM="\$(NELM=1024)"}
        {N=2, P="\$(SYS):\$(D)_CH$(N):", NAME="\$(NAME)", NELM="\$(NELM=1024)"}
        {N=3, P="\$(SYS):\$(D)_CH$(N):", NAME="\$(NAME)", NELM="\$(NELM=1024)"}
        {N=4, P="\$(SYS):\$(D)_CH$(N):", NAME="\$(NAME)", NELM="\$(NELM=1024)"}
        {N=5, P="\$(SYS):\$(D)_CH$(N):", NAME="\$(NAME)", NELM="\$(NELM=1024)"}
        {N=6, P="\$(SYS):\$(D)_CH$(N):", NAME="\$(NAME)", NELM="\$(NELM=1024)"}
        {N=7, P="\$(SYS):\$(D)_CH$(N):", NAME="\$(NAME)", NELM="\$(NELM=1024)"}
}

file "pico8_ram.template"
{
{P="\$(SYS):\$(D)_mem:", NAME="\$(NAME)", NELM="\$(NELM=1024)"}
}

file "pico8_frib_reg.db"
{
{NAME="\$(NAME)", CTRL="\$(SYS):\$(D)_CTRL:", FPS="\$(SYS):\$(D)_FPS:", SLOT="\$(SLOT=0)"}
}

file "pico8_cap_int.template"
{
{N="\$(SYS):\$(D)_CAP:SEC_RSTS", NAME="\$(NAME)", ADDR="0x0"}
{N="\$(SYS):\$(D)_CAP:NS_RSTS",  NAME="\$(NAME)", ADDR="0x4"}
{N="\$(SYS):\$(D)_CAP:TON_RSTS", NAME="\$(NAME)", ADDR="0x8"}
{N="\$(SYS):\$(D)_CAP:FLG_RSTS", NAME="\$(NAME)", ADDR="0xc"}
}

file "pico8_cap_float.template"
{
{P="\$(SYS):\$(D)_CH0:AVG", NAME="\$(NAME)", ADDR="0x10", Stat="Mean", PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH0:ASTD",NAME="\$(NAME)", ADDR="0x10", Stat="Std",  PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH0:MIN", NAME="\$(NAME)", ADDR="0x14", Stat="Min",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH0:MAX", NAME="\$(NAME)", ADDR="0x18", Stat="Max",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH0:STD", NAME="\$(NAME)", ADDR="0x1c", Stat="Mean",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH1:AVG", NAME="\$(NAME)", ADDR="0x24", Stat="Mean", PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH1:ASTD",NAME="\$(NAME)", ADDR="0x24", Stat="Std",  PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH1:MIN", NAME="\$(NAME)", ADDR="0x28", Stat="Min",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH1:MAX", NAME="\$(NAME)", ADDR="0x2c", Stat="Max",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH1:STD", NAME="\$(NAME)", ADDR="0x30", Stat="Mean",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH2:AVG", NAME="\$(NAME)", ADDR="0x38", Stat="Mean", PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH2:ASTD",NAME="\$(NAME)", ADDR="0x24", Stat="Std",  PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH2:MIN", NAME="\$(NAME)", ADDR="0x3c", Stat="Min",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH2:MAX", NAME="\$(NAME)", ADDR="0x40", Stat="Max",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH2:STD", NAME="\$(NAME)", ADDR="0x44", Stat="Mean",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH3:AVG", NAME="\$(NAME)", ADDR="0x4c", Stat="Mean", PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH3:ASTD",NAME="\$(NAME)", ADDR="0x4c", Stat="Std",  PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH3:MIN", NAME="\$(NAME)", ADDR="0x50", Stat="Min",  PREC="6", EGU="A"}
