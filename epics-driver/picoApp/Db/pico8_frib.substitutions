{P="\$(SYS):\$(D)_CH3:STD", NAME="\$(NAME)", ADDR="0x58", Stat="Mean",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH4:AVG", NAME="\$(NAME)", ADDR="0x60", Stat="Mean", PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH4:ASTD",NAME="\$(NAME)", ADDR="0x60", Stat="Std",  PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH4:MIN", NAME="\$(NAME)", ADDR="0x64", Stat="Min",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH4:MAX", NAME="\$(NAME)", ADDR="0x68", Stat="Max",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH4:STD", NAME="\$(NAME)", ADDR="0x6c", Stat="Mean",  PREC="6", EGU="A"}
{P="\$(SYS):\$(D)_CH5:AVG", NAME="\$(NAME)", ADDR="0x74", Stat="Mean", PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH5:ASTD",NAME="\$(NAME)", ADDR="0x74", Stat="Std",  PREC="6", EGU="A", ASLO="1e-4"}
{P="\$(SYS):\$(D)_CH5:MIN", NAME="\$(NAME)", ADDR="0x78", Stat="Min",  PREC="6", EGU="A"}
