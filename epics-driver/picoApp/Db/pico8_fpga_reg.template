record(ai, "$(P)$(R)_RD") {
    field(DTYP, "Pico8 FPGA Reg Float")
    field(INP,  "@$(NAME) offset=$(OFFSET)")
    field(PREC, "8")
    field(EGU,  "$(EGU=)")
    field(ASLO, "$(ASLO=)")
}

record(ao, "$(P)$(R)_CSET") {
    field(DTYP, "Pico8 FPGA Reg Float")
    field(OUT,  "@$(NAME) offset=$(OFFSET)")
    field(PREC, "8")
    field(FLNK, "$(P)$(R)_RD")
    field(SDIS, "$(P):FPGA_LOCK_CSET")
    field(DISV, "0")
    field(EGU,  "$(EGU=)")
    field(ASLO, "$(ASLO=)")
}
