
record(longout, "$(P)Off-SP") {
    field(DTYP, "Pico8 RAM Offset")
    field(OUT , "@$(NAME)")
}

record(longout, "$(P)Cnt-SP") {
    field(DTYP, "Pico8 RAM Count")
    field(OUT , "@$(NAME)")
}

record(waveform, "$(P)Data-I") {
    field(DTYP, "Pico8 RAM")
    field(FTVL, "$(FTVL=ULONG)")
    field(NELM, "$(NELM)")
    field(INP , "@$(NAME)")
}
